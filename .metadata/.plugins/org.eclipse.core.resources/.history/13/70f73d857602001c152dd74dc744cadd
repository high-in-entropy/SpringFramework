package com.viraj.javaconfig;

//import org.springframework.stereotype.Component;


//This tells Spring to make a java obj of class component.
//So in short this acts like a bean tag of XML but w/o the XML;
//@Component
//@Component isnt needed in case we are using the @Bean annotation.
public class Student {
	
	private Major major;
	
	

	public Student(Major major) {
		super();
		this.major = major;
	}

	public Major getMajor() {
		return major;
	}

	public void setMajor(Major major) {
		this.major = major;
	}

	@Override
	public String toString() {
		return "Student [major=" + major + "]";
	}
	
}
